{
 "doctype": "DocType",
 "name": "Saudization Dashboard Tab",
 "module": "Saudization Dashboard",
 "custom": 0,
 "istable": 1,
 "fields": [
  {
   "fieldname": "tab_label",
   "label": "Tab Label",
   "fieldtype": "Data",
   "reqd": 1
  },
  {
   "fieldname": "tab_type",
